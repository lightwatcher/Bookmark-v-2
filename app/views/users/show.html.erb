<h1>User #<%= @user.id %></h1>

<div>
  <strong>Name</strong>:<br />
  <p><%= @user.name %></p>
</div>
<div>
  <strong>Email</strong>:<br />
  <p><%= @user.email %></p>
</div>

<a href="/users/<%= @user.id %>/edit">Edit</a> |
<a href="/users/<%= @user.id %>/destroy">Destroy</a>
<h3>Bookmarks created by <%=@user.name%></h3>
  <ul>
  <% @bookmarks.each do |bookmark| %>
    <li><a href="/bookmarks/<%=bookmark.id%>"><%=bookmark.title%></a></li>
  <% end %>
</ul>
